       RowBox[{
        FractionBox[
         RowBox[{"2", "+", 
          SuperscriptBox["x0", "2"]}], 
         RowBox[{"1", "+", 
          SuperscriptBox["x0", "2"]}]], 
        SuperscriptBox[
         RowBox[{"(", 
