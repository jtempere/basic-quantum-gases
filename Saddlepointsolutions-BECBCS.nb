Cell[BoxData[
 RowBox[{
  RowBox[{"saddlepoint0", "[", " ", "x0_", " ", "]"}], ":=", 
  RowBox[{"Module", "[", " ", 
   RowBox[{"(*", 
    RowBox[{"input", " ", 
     FormBox[
      RowBox[{
       SubscriptBox["x", "0"], " ", "=", " ", 
       RowBox[{"\[CapitalDelta]", "/", "\[Mu]"}]}],
      TraditionalForm], " ", "and", " ", "returns", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[CapitalDelta]", "/", 
        FormBox[
         SubscriptBox["E", "F"],
         TraditionalForm]}], " ", ",", " ", 
       RowBox[{"\[Mu]", "/", 
        FormBox[
         SubscriptBox["E", "F"],
         TraditionalForm]}], " ", ",", " ", 
       RowBox[{"1", "/", 
        RowBox[{"(", 
         FormBox[
          RowBox[{
           SubscriptBox["k", "F"], 
           SubscriptBox["a", "s"]}],
          TraditionalForm], ")"}]}], ",", 
       RowBox[{
        FormBox[
         RowBox[{" ", 
          SubscriptBox["k", "F"]}],
         TraditionalForm], 
        FormBox[
         SubscriptBox["\[Xi]", "pair"],
         TraditionalForm]}], " ", ",", " ", 
       RowBox[{
        FormBox[
         SubscriptBox["k", "F"],
         TraditionalForm], 
        FormBox[
         SubscriptBox["\[Xi]", "phase"],
         TraditionalForm]}], " ", ",", " ", 
       RowBox[{"c", "/", 
        FormBox[
         SubscriptBox["v", "F"],
         TraditionalForm]}]}], "}"}]}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"ii6", "=", 
       RowBox[{
        FractionBox["1.", 
         RowBox[{"2", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"1", "+", 
             SuperscriptBox["x0", "2"]}], ")"}], 
           RowBox[{"1", "/", "4"}]]}]], 
        RowBox[{"EllipticF", "[", 
         RowBox[{
          FractionBox["Pi", "2"], ",", " ", 
          RowBox[{
           FractionBox["1", "2"], 
           RowBox[{"(", 
            RowBox[{"1", "+", 
             FractionBox["x0", 
              SqrtBox[
               RowBox[{"1", "+", 
                SuperscriptBox["x0", "2"]}]]]}], ")"}]}]}], "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"ii5", "=", 
       RowBox[{"With", "[", " ", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\[Kappa]", "=", 
           RowBox[{
            FractionBox["1", "2"], 
            RowBox[{"(", 
             RowBox[{"1", "+", 
              FractionBox["x0", 
               SqrtBox[
                RowBox[{"1", "+", 
                 SuperscriptBox["x0", "2"]}]]]}], ")"}]}]}], "}"}], ",", " ", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"1.", "+", 
              SuperscriptBox["x0", "2"]}], ")"}], 
            RowBox[{"1", "/", "4"}]], 
           RowBox[{"EllipticE", "[", 
            RowBox[{
             FractionBox["Pi", "2"], ",", "\[Kappa]"}], "]"}]}], "-", 
          RowBox[{
           FractionBox["1.", 
            RowBox[{"2", 
             RowBox[{"(", 
              RowBox[{
               SqrtBox[
                RowBox[{"1", "+", 
                 SuperscriptBox["x0", "2"]}]], "+", "x0"}], ")"}], 
             SuperscriptBox[
              RowBox[{"(", 
               RowBox[{"1", "+", 
                SuperscriptBox["x0", "2"]}], ")"}], 
              RowBox[{"1", "/", "4"}]]}]], 
           RowBox[{"EllipticF", "[", 
            RowBox[{
             FractionBox["Pi", "2"], ",", "\[Kappa]"}], "]"}]}]}]}], 
        "]"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{
         RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
       RowBox[{
        RowBox[{"-", "2"}], "/", "3"}]], ",", " ", "\[IndentingNewLine]", 
      RowBox[{"x0", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
        RowBox[{
         RowBox[{"-", "2"}], "/", "3"}]]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"-", 
        FractionBox["4", "\[Pi]"]}], 
       FractionBox[
        RowBox[{"(", 
         RowBox[{
          RowBox[{"x0", " ", "ii6"}], "-", "ii5"}], ")"}], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
         RowBox[{"1", "/", "3"}]]]}], ",", "\[IndentingNewLine]", 
      RowBox[{
       SqrtBox[
        RowBox[{
         FractionBox["1", "2"], 
         RowBox[{"(", 
          RowBox[{"1", "+", 
           SuperscriptBox[
            RowBox[{"(", 
             FractionBox[
              RowBox[{
               SqrtBox[
                RowBox[{"1", "+", 
                 SuperscriptBox["x0", "2"]}]], "+", "x0"}], "2"], ")"}], 
            "2"]}], ")"}]}]], 
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
         RowBox[{"1", "/", "3"}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "+", 
           SuperscriptBox["x0", "2"]}], ")"}], 
         RowBox[{"1", "/", "4"}]]]}], ",", "\[IndentingNewLine]", 
      SqrtBox[
       RowBox[{
        FractionBox[
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
          RowBox[{"2", "/", "3"}]], 
         RowBox[{"12", " ", "ii5"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", " ", "ii6"}], "+", 
          RowBox[{
           FractionBox[
            RowBox[{"1", "+", 
             RowBox[{"4", " ", 
              SuperscriptBox["x0", "2"]}]}], 
            RowBox[{"3", 
             RowBox[{"(", 
              RowBox[{"1", "+", 
               SuperscriptBox["x0", "2"]}], ")"}]}]], 
           RowBox[{"(", 
            RowBox[{"ii6", "+", 
             RowBox[{"x0", " ", "ii5"}]}], ")"}]}]}], ")"}]}]], ",", 
      "\[IndentingNewLine]", 
      SqrtBox[
       RowBox[{
        FractionBox["1", "3"], 
        FractionBox[
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             RowBox[{"x0", " ", "ii5"}], "+", "ii6"}], ")"}], 
           RowBox[{"1", "/", "3"}]], " ", "ii5"}], 
         RowBox[{
          SuperscriptBox["ii5", "2"], "+", 
          SuperscriptBox["ii6", "2"]}]]}]]}], "}"}]}], "]"}]}]], "Input",
