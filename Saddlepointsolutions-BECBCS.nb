Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"i6", "[", "x0_", "]"}], ":=", 
   RowBox[{
    FractionBox["1.", 
     RowBox[{"2", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"1", "+", 
         SuperscriptBox["x0", "2"]}], ")"}], 
       RowBox[{"1", "/", "4"}]]}]], 
    RowBox[{"EllipticF", "[", 
     RowBox[{
      FractionBox["Pi", "2"], ",", " ", 
      RowBox[{
       FractionBox["1", "2"], 
       RowBox[{"(", 
        RowBox[{"1", "+", 
         FractionBox["x0", 
          SqrtBox[
           RowBox[{"1", "+", 
            SuperscriptBox["x0", "2"]}]]]}], ")"}]}]}], "]"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"i5", "[", "x0_", "]"}], ":=", 
   RowBox[{"With", "[", " ", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Kappa]", "=", 
       RowBox[{
        FractionBox["1", "2"], 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          FractionBox["x0", 
           SqrtBox[
            RowBox[{"1", "+", 
             SuperscriptBox["x0", "2"]}]]]}], ")"}]}]}], "}"}], ",", " ", 
     RowBox[{
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"1.", "+", 
          SuperscriptBox["x0", "2"]}], ")"}], 
        RowBox[{"1", "/", "4"}]], 
       RowBox[{"EllipticE", "[", 
        RowBox[{
         FractionBox["Pi", "2"], ",", "\[Kappa]"}], "]"}]}], "-", 
      RowBox[{
       FractionBox["1.", 
        RowBox[{"2", 
         RowBox[{"(", 
          RowBox[{
           SqrtBox[
            RowBox[{"1", "+", 
             SuperscriptBox["x0", "2"]}]], "+", "x0"}], ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"1", "+", 
            SuperscriptBox["x0", "2"]}], ")"}], 
          RowBox[{"1", "/", "4"}]]}]], 
       RowBox[{"EllipticF", "[", 
        RowBox[{
         FractionBox["Pi", "2"], ",", "\[Kappa]"}], "]"}]}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalDelta]over\[Mu]", "[", " ", "g_", " ", "]"}], ":=", 
  RowBox[{"(", 
   RowBox[{"x", " ", "/.", " ", 
    RowBox[{"FindRoot", "[", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"-", 
         FractionBox["4", "\[Pi]"]}], 
        FractionBox[
         RowBox[{"(", 
          RowBox[{
           RowBox[{"x", " ", 
            RowBox[{"i6", "[", "x", "]"}]}], "-", 
           RowBox[{"i5", "[", "x", "]"}]}], ")"}], 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            RowBox[{"x", " ", 
             RowBox[{"i5", "[", "x", "]"}]}], "+", 
            RowBox[{"i6", "[", "x", "]"}]}], ")"}], 
          RowBox[{"1", "/", "3"}]]]}], "\[Equal]", "g"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"x", ",", 
        RowBox[{"-", "1"}], ",", "5"}], "}"}]}], "]"}]}], ")"}]}]}], "Input",
