 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalDelta]2D", "[", "x0_", "]"}], ":=", 
   FractionBox["2", 
    RowBox[{
     SqrtBox[
      RowBox[{"1", "+", 
       SuperscriptBox["x0", "2"]}]], "+", "x0"}]]}], ";"}]], "Input",
 CellChangeTimes->{{3.8731766580391707`*^9, 3.873176704207142*^9}, 
   3.873176802518261*^9, 3.873179674996275*^9, {3.874924772923727*^9, 
   3.874924775818925*^9}, 3.8749248712609715`*^9, {3.8749252612908306`*^9, 
   3.87492526189923*^9}},
 CellLabel->"In[2]:=",ExpressionUUID->"53d429e7-47ea-41a7-97e0-8458fd99b14f"],

Cell["Next, we have the chemical potential :", "Text",
 CellChangeTimes->{{3.873176814248021*^9, 
  3.8731768284220047`*^9}},ExpressionUUID->"1338b781-74c3-4259-a79b-\
32f43eec9dd4"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"\[Mu]2D", "[", "x0_", "]"}], ":=", 
   RowBox[{"x0", 
    FractionBox["2", 
     RowBox[{
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox["x0", "2"]}]], "+", "x0"}]]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.8731767142783904`*^9, 3.8731767262551613`*^9}, 
   3.8731768015420303`*^9, 3.873179677444273*^9, 3.8749249608757954`*^9, {
   3.874925262913845*^9, 3.874925263465845*^9}},
 CellLabel->"In[3]:=",ExpressionUUID->"954dae8b-ba63-44fd-96cd-24fb7a26a00b"],

Cell[TextData[{
 "Then, the bound state energy ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "b"], TraditionalForm]],ExpressionUUID->
  "b194c8d7-a50c-4324-83e9-d9028cfa849c"],
 StyleBox["/",
  FontWeight->"Bold"],
 Cell[BoxData[
  FormBox[
   SubscriptBox["E", "F"], TraditionalForm]],ExpressionUUID->
  "37b8b31a-d24a-4624-bb9f-933a74de9e98"],
 " :"
}], "Text",
 CellChangeTimes->{{3.8731768568960824`*^9, 3.8731768674709706`*^9}, {
  3.874924907819904*^9, 
  3.8749249208844643`*^9}},ExpressionUUID->"c9130dd0-3386-4604-b013-\
fd93b4b1e28a"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"ebEf", "[", "x0_", "]"}], ":=", 
   RowBox[{"2", 
    FractionBox[
     RowBox[{
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox["x0", "2"]}]], "-", "x0"}], 
     RowBox[{
      SqrtBox[
       RowBox[{"1", "+", 
        SuperscriptBox["x0", "2"]}]], "+", "x0"}]]}]}], ";"}]], "Input",
 CellChangeTimes->{{3.873176737696973*^9, 3.8731767997510347`*^9}, 
   3.8731796786840367`*^9, {3.8749249033565006`*^9, 3.8749249438278484`*^9}},
 CellLabel->"In[4]:=",ExpressionUUID->"c18de1d6-204e-4d86-a3ec-b0efad0fd869"],

Cell[TextData[{
 "The pair correlation length ",
 Cell[BoxData[
  RowBox[{
   SubscriptBox["k", "F"], 
   SubscriptBox["\[Xi]", "pair"]}]],ExpressionUUID->
  "0dbeb3b4-34e1-46e6-b231-db604eeb76d7"],
 "  is :"
}], "Text",
